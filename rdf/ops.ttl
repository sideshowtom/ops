@prefix bfo: <http://www.ifomis.org/bfo/1.1#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix ops: <http://purl.org/socialparticipation/ops/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix snap: <http://www.ifomis.org/bfo/1.1/snap#> .
@prefix span: <http://www.ifomis.org/bfo/1.1/span#> .
@prefix vs: <http://www.w3.org/2003/06/sw-vocab-status/ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

ops:Action a rdfs:Class,
        owl:Class ;
    rdfs:label "Action"@en,
        "Acción"@es,
        "Ação"@pt ;
    rdfs:comment "what is done in terms os social participation"@en ;
    rdfs:subClassOf span:ProcessualEntity .

ops:Cause a rdfs:Class,
        owl:Class ;
    rdfs:label "Cause"@en,
        "Causa"@es,
        "Causa"@pt ;
    rdfs:comment "the motivation for Action"@en ;
    rdfs:subClassOf snap:IndependentContinuant .

ops:Executor a rdfs:Class,
        owl:Class ;
    rdfs:label "Executor"@en,
        "Ejecutor"@es,
        "Executor"@pt ;
    rdfs:comment "performs action directly and is responsible for results"@en ;
    rdfs:subClassOf ops:SocialActor,
        snap:MaterialEntity .

ops:Initiator a rdfs:Class,
        owl:Class ;
    rdfs:label "Initiator"@en,
        "Iniciador"@es,
        "Iniciador"@pt ;
    rdfs:comment "originates cause, individually or collaborativelly"@en ;
    rdfs:subClassOf ops:SocialActor,
        snap:MaterialEntity .

ops:Organization a rdfs:Class,
        owl:Class ;
    rdfs:label "Organization"@en,
        "Organización"@es,
        "Organização"@pt ;
    rdfs:comment "social actor is a group of individuals, organized formally or informally (e.g. movements, collectives)"@en ;
    rdfs:subClassOf ops:SocialActor,
        snap:MaterialEntity,
        foaf:Organization ;
    owl:disjointWith ops:Person .

ops:ParticipationCharacteristic a rdfs:Class,
        owl:Class ;
    rdfs:label "Participation Characteristic"@en,
        "Característica de Participación"@es,
        "Característica de Participação"@pt ;
    rdfs:comment "the way the participation of specific actor is happening"@en ;
    rdfs:subClassOf snap:DependentContinuant .

ops:Person a rdfs:Class,
        owl:Class ;
    rdfs:label "Person"@en,
        "Persona"@es,
        "Pessoa"@pt ;
    rdfs:comment "a person (social actor is a person)"@en ;
    rdfs:subClassOf ops:SocialActor,
        snap:MaterialEntity,
        foaf:Person .

ops:Problem a rdfs:Class,
        owl:Class ;
    rdfs:label "Problem"@en,
        "Problema"@es,
        "Problema"@pt ;
    rdfs:comment "the problem that the Action aims to solve"@en ;
    rdfs:subClassOf snap:IndependentContinuant .

ops:Result a rdfs:Class,
        owl:Class ;
    rdfs:label "Result"@en,
        "Resultado"@es,
        "Resultado"@pt ;
    rdfs:comment "the result obtained with Action"@en ;
    rdfs:subClassOf snap:DependentContinuant .

ops:Scope a rdfs:Class,
        owl:Class ;
    rdfs:label "Scope"@en,
        "Ambito"@es,
        "Escopo"@pt ;
    rdfs:comment "the scope of Action"@en ;
    rdfs:subClassOf snap:DependentContinuant .

ops:SocialActor a rdfs:Class,
        owl:Class ;
    rdfs:label "Social Actor"@en,
        "Actor Social"@es,
        "Ator Social"@pt ;
    rdfs:comment "entity that might have a participatory role"@en ;
    rdfs:subClassOf snap:MaterialEntity .

ops:Solution a rdfs:Class,
        owl:Class ;
    rdfs:label "Solution"@en,
        "Solución"@es,
        "Solução"@pt ;
    rdfs:comment "solution achieved with Action"@en ;
    rdfs:subClassOf snap:DependentContinuant .

ops:Supporter a rdfs:Class,
        owl:Class ;
    rdfs:label "Supporter"@en,
        "Apoyador"@es,
        "Apoiador"@pt ;
    rdfs:comment "supports cause with resources of any kind (e.g. cognitive, financial, equipments)"@en ;
    rdfs:subClassOf ops:SocialActor,
        snap:MaterialEntity .

ops:Theme a rdfs:Class,
        owl:Class ;
    rdfs:label "Theme"@en,
        "Tema"@es,
        "Tema"@pt ;
    rdfs:comment "the theme in focus by Action"@en ;
    rdfs:subClassOf snap:IndependentContinuant .

foaf:Organization a rdfs:Class,
        owl:Class ;
    rdfs:label "Organization" ;
    rdfs:comment "An organization." ;
    rdfs:isDefinedBy foaf: ;
    rdfs:subClassOf foaf:Agent ;
    owl:disjointWith foaf:Document,
        foaf:Person ;
    vs:term_status "stable" .

foaf:Person a rdfs:Class,
        owl:Class ;
    rdfs:label "Person" ;
    rdfs:comment "A person." ;
    rdfs:isDefinedBy foaf: ;
    rdfs:subClassOf <http://www.w3.org/2003/01/geo/wgs84_pos#SpatialThing>,
        foaf:Agent ;
    owl:disjointWith foaf:Organization,
        foaf:Project ;
    owl:equivalentClass <http://schema.org/Person>,
        <http://www.w3.org/2000/10/swap/pim/contact#Person> ;
    vs:term_status "stable" .

ops:action a rdf:Property,
        owl:ObjectProperty ;
    rdfs:label "action"@en,
        "acción"@es,
        "ação"@pt .

ops:belongsTo a rdf:Property,
        owl:ObjectProperty ;
    rdfs:label "belongs to"@en,
        "pertence al"@es,
        "pertence ao"@pt ;
    rdfs:range ops:Scope .

ops:contributesTo a rdf:Property,
        owl:ObjectProperty ;
    rdfs:label "contributes to"@en,
        "contribuye a la"@es,
        "contribui para"@pt .

ops:executes a rdf:Property,
        owl:ObjectProperty ;
    rdfs:label "executes"@en,
        "ejecuta"@es,
        "executa"@pt .

ops:generates a rdf:Property,
        owl:ObjectProperty ;
    rdfs:label "generates"@en,
        "genera"@es,
        "gera"@pt .

ops:ops.owl a owl:Ontology ;
    rdfs:label "OPS"^^xsd:string,
        "Social Participation Ontology"@en,
        "Ontología de la participación social"@es,
        "Ontologia de Participação Social"@pt ;
    rdfs:comment "Social Participation Ontology headed by Cidade Democrática, Brazilian Federal Government and Corais Platform, in direct collaboration with dozens of actors throughout latin america"@en,
        "Ontología de Participación Social conducida por el Cidade Democrática, el Gobierno Federal de Brasil y la Plataforma Corais, en colaboración directa con docenas de actores en toda América Latina"@es,
        "Ontologia de Participação Social capitaneada pelo Cidade Democrática, Governo Federal Brazileiro e Plataforma Corais, em colaboração direta com dezenas de agentes de toda américa latina"@pt ;
    rdfs:seeAlso <http://purl.org/socialparticipation/aa>,
        <http://purl.org/socialparticipation/obs>,
        <http://purl.org/socialparticipation/ocd>,
        <http://purl.org/socialparticipation/opa>,
        <http://purl.org/socialparticipation/vbs> ;
    owl:versionInfo "0.02"^^xsd:string .

ops:produces a rdf:Property,
        owl:ObjectProperty ;
    rdfs:label "produces"@en,
        "produce"@es,
        "produz"@pt .

ops:proposes a rdf:Property,
        owl:ObjectProperty ;
    rdfs:label "proposes"@en,
        "propone"@es,
        "propõe"@pt .

ops:solves a rdf:Property,
        owl:ObjectProperty ;
    rdfs:label "solves"@en,
        "resuelve"@es,
        "soluciona"@pt .

ops:starts a rdf:Property,
        owl:ObjectProperty ;
    rdfs:label "starts"@en,
        "inicia"@es,
        "inicia"@pt .

ops:supports a rdf:Property,
        owl:ObjectProperty ;
    rdfs:label "supports"@en,
        "apoya"@es,
        "apoia"@pt .

ops:theme a rdf:Property,
        owl:ObjectProperty ;
    rdfs:label "theme"@en,
        "tema"@es,
        "tema"@pt ;
    rdfs:range ops:Theme .

ops:trait a rdf:Property,
        owl:ObjectProperty ;
    rdfs:label "trait"@en,
        "rasgo"@es,
        "traço"@pt .

snap:FiatObjectPart a owl:Class .

snap:GenericallyDependentContinuant a owl:Class .

snap:Object a owl:Class .

snap:ObjectAggregate a owl:Class .

snap:SpecificallyDependentContinuant a owl:Class .

span:FiatProcessPart a owl:Class .

span:Process a owl:Class .

span:ProcessAggregate a owl:Class .

span:ProcessBoundary a owl:Class .

span:ProcessualContext a owl:Class .

span:ProcessualEntity a owl:Class ;
    rdfs:label "processual_entity"^^xsd:string ;
    rdfs:comment "Definition: An occurrent [span:Occurrent] that exists in time by occurring or happening, has temporal parts and always involves and depends on some entity."^^xsd:string,
        "Examples: the life of an organism, the process of meiosis, the course of a disease, the flight of a bird"^^xsd:string ;
    rdfs:subClassOf span:Occurrent ;
    owl:disjointWith span:SpatiotemporalRegion,
        span:TemporalRegion ;
    owl:equivalentClass [ a owl:Class ;
            owl:unionOf ( span:FiatProcessPart span:Process span:ProcessAggregate span:ProcessBoundary span:ProcessualContext ) ] .

<http://www.w3.org/2003/01/geo/wgs84_pos#SpatialThing> a owl:Class ;
    rdfs:label "Spatial Thing" .

snap:ObjectBoundary a owl:Class .

snap:Site a owl:Class .

foaf:Agent a owl:Class .

snap:DependentContinuant a owl:Class ;
    rdfs:label "dependent_continuant"^^xsd:string ;
    rdfs:comment "Definition: A continuant [snap:Continuant] that is either dependent on one or other independent continuant [snap:IndependentContinuant] bearers or inheres in or is borne by other entities."^^xsd:string ;
    rdfs:subClassOf snap:Continuant ;
    owl:disjointWith snap:IndependentContinuant,
        snap:SpatialRegion ;
    owl:equivalentClass [ a owl:Class ;
            owl:unionOf ( snap:GenericallyDependentContinuant snap:SpecificallyDependentContinuant ) ] .

snap:IndependentContinuant a owl:Class ;
    rdfs:label "independent_continuant"^^xsd:string ;
    rdfs:comment "Definition: A continuant [snap:Continuant] that is a bearer of quality [snap:Quality] and realizable entity [snap:RealizableEntity] entities, in which other entities inhere and which itself cannot inhere in anything."^^xsd:string,
        "Examples: an organism, a heart, a leg, a person, a symphony orchestra, a chair, the bottom right portion of a human torso, the lawn and atmosphere in front of our building"^^xsd:string,
        "Synonyms: substantial entity"^^xsd:string ;
    rdfs:subClassOf snap:Continuant ;
    owl:disjointWith snap:DependentContinuant,
        snap:SpatialRegion ;
    owl:equivalentClass [ a owl:Class ;
            owl:unionOf ( snap:MaterialEntity snap:ObjectBoundary snap:Site ) ] .

snap:MaterialEntity a owl:Class ;
    rdfs:label "material_entity"^^xsd:string ;
    rdfs:comment "Definition: An independent continuant [snap:IndependentContinuant] that is spatially extended whose identity is independent of that of other entities and can be maintained through time. Note: Material entity [snap:MaterialEntity] subsumes object [snap:Object], fiat object part [snap:FiatObjectPart], and object aggregate [snap:ObjectAggregate], which assume a three level theory of granularity, which is inadequate for some domains, such as biology."^^xsd:string,
        "Examples: collection of random bacteria, a chair, dorsal surface of the body"^^xsd:string ;
    rdfs:subClassOf snap:IndependentContinuant ;
    owl:disjointWith snap:ObjectBoundary,
        snap:Site ;
    owl:equivalentClass [ a owl:Class ;
            owl:unionOf ( snap:FiatObjectPart snap:Object snap:ObjectAggregate ) ] .

